  <style>
    .main-container {
      display: flex;
      align-items: center;
      justify-content: center;
      width: 70%;
      margin-left: 10em;
      border-radius: 1em;
    }

    .container {
      position: relative;
      width: 12em;
      height: 12em;
      margin: 2em auto;
      border-radius: 1em;
    }

    .card {
      position: absolute;
      width: 100%;
      height: 100%;
      transform-style: preserve-3d;
      transition: all 0.5s ease;
      border-radius: 1em;
    }

    .card:hover {
      transform: rotateX(180deg);
    }

    .front {
      position: absolute;
      width: 100%;
      height: 100%;
      backface-visibility: hidden;
      background-color: #343a40;
      color: #fad0c9;
      border-radius: 1em;
    }

    .back {
      position: absolute;
      width: 100%;
      height: 100%;
      backface-visibility: hidden;
      background-color: #fad0c9;
      color: #343a40;
      transform: rotateX(180deg);
      border-radius: 1em;
    }

    .center {
      margin: auto;
      display: flex;
      justify-content: center;
      align-items: center;
      align-self: center;
    }

    table {
      margin: 10px;
      padding: 5px;
      text-align: center;
    }

    table,
    td,
    th {
      border: 1px solid black;
      border-collapse: collapse;
      padding: 0.5em;
    }

    .container {
      display: flex;
      justify-content: center;
    }

    .text-center{
      padding: 1em;
      text-align: center;
    }
  </style>
</head>

